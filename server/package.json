{
  "name": "server",
  "version": "1.3.0",
  "private": true,
  "main": "./build/index.js",
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --files src/database/seed.ts"
  },
  "scripts": {
    "start": "nodemon --exec ts-node -r tsconfig-paths/register --files ./src/index.ts",
    "start:production": "cross-env NODE_ENV=production TS_NODE_BASEURL=build node -r tsconfig-paths/register build/index.js",
    "build": "rimraf build && tsc",
    "test": "jest --config jest.config.js --runInBand --forceExit",
    "sync-env": "sync-dotenv",
    "db:restore": "prisma migrate dev",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "format": "prettier --write '**/*.{js,ts,json,css,scss,html}'"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^3.1.1",
    "ms": "^2.1.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.191",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.11.17",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "prisma": "^4.7.1",
    "rimraf": "^3.0.2",
    "sync-dotenv": "^2.7.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}